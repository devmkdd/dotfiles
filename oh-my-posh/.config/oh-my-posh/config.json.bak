{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "closer": "p:os",
        "pink": "#F4B8E4",
        "lavender": "#BABBF1",
        "blue":  "#8CAAEE",
				"peach": "#ef9f76",
				"base": "#303446",
				"yellow": "#e5c890",
				"green": "#a6d189"
  },
	"blocks": [
				{
			"alignment": "right",
			"segments": [
				{
          "foreground": "p:green",
          "properties": {
            "branch_gone_icon": "\u274e",
            "branch_identical_icon": "\uf14a"
          },
          "style": "plain",
          "template": " {{ .HEAD }}",
          "type": "git"
        }
			],
			"type": "prompt"
		},
		{
      "alignment": "left",
			"newline": true,
      "segments": [
				{
          "foreground": "p:yellow",
          "properties": {
            "style": "agnostic_short"
          },
          "style": "diamond",
          "template": "{{ .Path }}",
          "type": "path"
        },
								{
          "foreground": "p:pink",
          "style": "diamond",
          "template": "\u276f",
          "trailing_diamond": "",
          "type": "text"
        }
			],
			"type": "prompt"
		}
			],
			"type": "prompt"
		}

	],
	"final_space": true,
	"version": 2
}
